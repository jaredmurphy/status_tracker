
<% if @current_user %>
  <%= link_to "Statuses", statuses_path %>
  <%= link_to "Sign out", destroy_sessions_path, method: :delete %>
  <%= render "dashboard", dashboard: @dashboard %>
<% else %>
  <%= link_to "Sign up", new_registrations_path %>
  <%= link_to "Sign in", new_logins_path %>
<% end %>

